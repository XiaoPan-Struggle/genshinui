{
  "name": "genshinui",
  "version": "0.1.3",
  "description": "React component library",
  "author": "XiaoPan",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React",
    "genshinui"
  ],
  "homepage": "https://github.com/XiaoPan-Struggle/genshinui",
  "repository": {
    "type": "git",
    "url": "https://github.com/XiaoPan-Struggle/genshinui"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.3.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "dist": "react-scripts dist",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "building": "yarn rmrf && yarn buildts && yarn buildcss",
    "rmrf": "rimraf ./dist",
    "buildts": "tsc -p tsconfig.build.json",
    "buildcss": "node-sass ./src/styles/index.scss ./dist/index.css",
    "prepublish": "yarn test:nowatch && yarn building"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "cross-env": "^7.0.3",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  }
}
